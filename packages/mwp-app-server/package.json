    "hapi": "18.1.0",
    "inert": "5.1.3",
    "mwp-api-proxy-plugin": ">=0.0.1",
    "mwp-app-route-plugin": ">=0.0.1",
    "mwp-auth-plugin": ">=0.0.1",
