{
  "name": "meetup-web-platform",
  "version": "0.0.1",
  "description": "A collection of Node web application utilities for the Meetup web platform",
  "engineStrict": true,
  "engines": {
    "node": ">=8.9.0"
  },
  "main": "./lib/index.js",
  "config": {
    "description": "This config is used to mock a consumer app config",
    "subdomain": "mwp-foo",
    "agent": "MUP_WEB"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "lint": "eslint . --ext .js,.jsx --fix",
    "precommit": "lint-staged",
    "typecheck": "flow",
    "test": "yarn bootstrap && yarn build && yarn test:unit",
    "test:unit": "NODE_ENV=test jest --config jest.config.json",
    "test:integration": "NODE_ENV=test jest --config jest.int.config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meetup/meetup-web-platform.git"
  },
  "author": "Meetup",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meetup/meetup-web-platform/issues"
  },
  "homepage": "https://github.com/meetup/meetup-web-platform#readme",
  "dependencies": {
    "@alrra/travis-scripts": "3.0.1",
    "lerna": "2.9.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "21.0.0",
    "babel-plugin-dynamic-import-node": "1.0.2",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "boom": "5.2.0",
    "electrode-csrf-jwt": "1.2.1",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "1.0.2",
    "enzyme-to-json": "3.3.1",
    "eslint": "4.2.0",
    "eslint-config-prettier": "2.3.0",
    "eslint-plugin-flowtype": "2.34.1",
    "eslint-plugin-react": "7.1.0",
    "flow-bin": "0.66.0",
    "hapi": "16.5.2",
    "husky": "0.14.3",
    "inert": "4.2.1",
    "intl-messageformat": "2.1.0",
    "intl-relativeformat": "2.0.0",
    "iron": "4.0.5",
    "jest": "20.0.4",
    "joi": "13.0.1",
    "lint-staged": "4.0.1",
    "meetup-web-components": "3.2.1592",
    "meetup-web-mocks": "1.0.217",
    "memwatch-next": "0.3.0",
    "mwp-router": "11.1.2034",
    "newrelic": "3.2.0",
    "node-fetch": "1.7.2",
    "prettier": "1.5.2",
    "prop-types": "15.6.0",
    "querystring": "0.2.0",
    "react": "16.2.0",
    "react-dom": "16.0.0",
    "react-helmet": "5.2.0",
    "react-intl": "2.4.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "react-test-renderer": "16.0.0",
    "react-transition-group": "2.2.1",
    "redux": "4.0.0",
    "request": "2.83.0",
    "reselect": "3.0.1",
    "rison": "0.1.1",
    "rxjs": "5.5.1",
    "swarm-icons": "1.3.259",
    "tough-cookie": "2.3.2"
  },
  "lint-staged": {
    "{tests,__mocks__,packages}/**/*.{js,jsx}": [
      "prettier --write --single-quote --use-tabs --trailing-comma es5",
      "eslint",
      "git add"
    ]
  }
}
