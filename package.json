{
  "name": "meetup-web-platform",
  "version": "0.0.0",
  "description": "A collection of Node web application utilities developed for the Meetup web platform",
  "engineStrict": true,
  "engines": {
    "node": "^7.0.0"
  },
  "main": "./lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib --ignore test.js,test.jsx --source-maps",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "lint": "eslint . --ext .js,.jsx --fix",
    "prepublish": "yarn check && yarn run build",
    "test": "yarn run test:unit && yarn run test:integration",
    "test:unit": "jest --silent --config jest.config.json --coverage",
    "test:integration": "jest --silent --config jest.int.config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meetup/meetup-web-platform.git"
  },
  "author": "Meetup",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meetup/meetup-web-platform/issues"
  },
  "homepage": "https://github.com/meetup/meetup-web-platform#readme",
  "dependencies": {
    "accepts": "1.3.3",
    "avsc": "4.1.11",
    "boom": "4.2.0",
    "chalk": "1.1.3",
    "cookie": "0.3.1",
    "electrode-csrf-jwt": "1.2.1",
    "escape-html": "1.0.3",
    "good": "7.1.0",
    "good-console": "6.4.0",
    "good-squeeze": "5.0.1",
    "hapi": "16.1.0",
    "history": "3.2.1",
    "idb-keyval": "2.3.0",
    "intl": "1.2.5",
    "joi": "10.2.1",
    "js-cookie": "2.1.3",
    "node-fetch": "1.6.3",
    "qs": "6.4.0",
    "react-dom": "15.4.2",
    "react-router-redux": "4.0.8",
    "react-side-effect": "1.0.2",
    "redbox-react": "1.3.3",
    "redux-observable": "0.14.0",
    "request": "2.80.0",
    "source-map-support": "0.4.11",
    "tough-cookie": "2.3.2",
    "url-regex": "3.2.0",
    "uuid": "3.0.1"
  },
  "peerDependencies": {
    "react": "^15.4.2",
    "react-helmet": ">3.3.2",
    "react-intl": "^2.2.3",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "redux": "^3.5.2",
    "rxjs": "^5.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "coveralls": "2.12.0",
    "eslint": "3.17.0",
    "eslint-plugin-react": "6.10.0",
    "intl-messageformat": "1.3.0",
    "intl-relativeformat": "1.3.0",
    "iron": "4.0.4",
    "jest-cli": "19.0.2",
    "meetup-web-mocks": "0.1.68",
    "react": "15.4.2",
    "react-addons-test-utils": "15.4.2",
    "react-helmet": "4.0.0",
    "react-intl": "2.2.3",
    "react-redux": "5.0.3",
    "react-router": "3.0.2",
    "redux": "3.5.2",
    "rxjs": "5.2.0",
    "tough-cookie": "2.3.2"
  }
}
